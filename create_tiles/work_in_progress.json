{
  "config": {
    "output_file": "FAIL",
    "output_directory": "/opt/nautoguide/fast_mount/vector_tiles/output",
    "tmp_dir": "/opt/nautoguide/fast_mount/vector_tiles/tmp/",
    "minzoom": 0,
    "maxzoom": 12,
    "detail_level": 11,
    "merge_shapefiles": [
      {
         "name": "uk_openroads",
         "search_path": "/opt/nautoguide/fast_mount/vector_tiles/openroads/data",
         "regex": "[A-Z]{2}_RoadLink\\.shp",
         "output_file": "/opt/nautoguide/fast_mount/vector_tiles/openroads/data/openroads_all_roads.shp",
         "overwrite" : "n"
      },
      {
        "name": "uk_water_line",
        "search_path": "/opt/nautoguide/fast_mount/vector_tiles/openmap/data",
        "regex": "[A-Z]{2}_SurfaceWater_Line\\.shp",
        "output_file": "/opt/nautoguide/fast_mount/vector_tiles/openroads/data/openmap_all_surfacewater_line.shp",
        "overwrite" : "n"
      },
      {
        "name": "uk_water_area",
        "search_path": "/opt/nautoguide/fast_mount/vector_tiles/openmap/data",
        "regex": "[A-Z]{2}_SurfaceWater_Area\\.shp",
        "output_file": "/opt/nautoguide/fast_mount/vector_tiles/openroads/data/openmap_all_surfacewater_area.shp",
        "overwrite" : "n"
      }
    ]
  },
  "layers": {
    "world_coast": [
      {
        "name": "World Coast",
        "file": "/opt/nautoguide/fast_mount/vector_tiles/land_shapes/data/world_with_os_mercator.shp",
        "s_srs": "EPSG:3857",
        "maxzoom": 6,
        "minzoom": 0,
        "add": {
          "name": "coast"
        },
        "skip" : "n"
      },
      {
        "name": "UK Coast",
        "file": "/opt/nautoguide/fast_mount/vector_tiles/land_shapes/data/uk_coast.shp",
        "maxzoom": 16,
        "minzoom": 7,
        "add": {
          "name": "coast"
        },
        "skip" : "n"
      }
    ],
    "national_park": [
      {
        "name": "National Park",
        "file": "/opt/nautoguide/gisraw/os-opendata-2016/strategi/strtgi_essh_gb/data/national_park.shp",
        "maxzoom": 13,
        "minzoom": 7,
        "attrs": [
          "code,legend,name"
        ]
      }
    ],
    "uk_motorway_overview": [
      {
        "name": "UK Motorway Overview",
        "file": "/opt/nautoguide/fast_mount/vector_tiles/openroads/data/openroads_all_roads.shp",
        "maxzoom": 13,
        "minzoom": 8,
        "selector" : "class = 'Motorway' AND formofway != 'Slip Road'",
        "attrs": [
          "roadnumber", "class", "formOfWay"
        ]
      }
    ],
    "uk_primary_overview": [
      {
        "name": "UK Primary Overview",
        "file": "/opt/nautoguide/fast_mount/vector_tiles/openroads/data/openroads_all_roads.shp",
        "maxzoom": 13,
        "minzoom": 8,
        "selector" : "class = 'A Road' AND formofway != 'Slip Road'",
        "attrs": [
          "roadnumber", "class", "formOfWay"
        ]
      }
    ],
    "uk_secondary_overview": [
      {
        "name": "UK Secondary Overview",
        "file": "/opt/nautoguide/fast_mount/vector_tiles/openroads/data/openroads_all_roads.shp",
        "maxzoom": 13,
        "minzoom": 10,
        "selector" : "(class = 'B Road' OR class = 'Classified Unnumbered') AND formofway != 'Slip Road'",
        "attrs": [
          "roadnumber", "class", "formOfWay"
        ]
      }
    ],
    "uk_unclassified_overview": [
      {
        "name": "UK Unclassified Overview",
        "file": "/opt/nautoguide/fast_mount/vector_tiles/openroads/data/openroads_all_roads.shp",
        "maxzoom": 13,
        "minzoom": 11,
        "selector" : "(class = 'Not Classified' OR class = 'Unclassified') AND formofway != 'Slip Road'",
        "attrs": [
          "roadnumber", "class", "formOfWay"
        ]
      }
    ]
  }
}